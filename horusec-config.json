    "horusecCliEnableCommitAuthor": true,
    "horusecCliEnableGitHistoryAnalysis": true,
    "horusecCliEnableInformationSeverity": true,
    "horusecCliFilesOrPathsToIgnore": [
      "*tmp*",
      "*__reactpreview__*",
      "*data*",
      "*.github*",
